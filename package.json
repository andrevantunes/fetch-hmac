{
  "name": "@andrevantunes/fetch-hmac",
  "version": "2.4.9",
  "description": "Lib para realizar os requests para a API de qualquer aplicação javascript, implementando HMAC e Serializer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --updateSnapshot",
    "test:watch": "jest --watch",
    "coverage": "jest --updateSnapshot --coverage",
    "build": "bin/build.sh",
    "build:export": "cp -r dist ../front-lps/node_modules/@mesalva/api",
    "lint": "prettier --write \"{src,__tests__}/**/*.{ts,js}\"",
    "lint:check": "prettier -l \"{src,__tests__}/**/*.{ts,js}\"",
    "prepare": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mesalva/js-api-request.git"
  },
  "license": "ISC",
  "keywords": [
    "Me Salva!",
    "HMAC",
    "fetch",
    "JSON",
    "API"
  ],
  "author": "Andre Antunes Vieira",
  "dependencies": {
    "cross-fetch": "3.1.5",
    "crypto-js": "3.1.9-1",
    "encoding": "0.1.13",
    "express-useragent": "1.0.8",
    "js-cookie": "^2.2.1",
    "jsonapi-serializer": "3.5.5"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.0.1",
    "@types/jest": "^26.0.15",
    "@types/js-cookie": "^2.2.6",
    "@types/jsonapi-serializer": "^3.6.3",
    "@types/node": "^14.14.3",
    "dts-bundle": "^0.7.3",
    "jest": "^26.6.1",
    "prettier": "^2.1.2",
    "rollup": "^2.32.1",
    "ts-jest": "^26.4.2",
    "typescript": "^4.0.3",
    "uglify-js": "^3.11.4"
  }
}
